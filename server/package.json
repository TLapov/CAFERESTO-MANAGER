  "main": "server.js",
  "scripts": {
    "build": "rm -rf ./build && tsc --build",
    "build:migrate": "rm -rf ./build-migration && tsc -p ./src/migration/tsconfig.migration.json",
    "dev": "nodemon server.ts",
    "migrate:dev": "nodemon ./src/migration/index.migration.ts",
    "start": "node ./build/server",
    "migrate:init": "node ./build-migration/migration/index.migration --init",
    "migrate": "node ./build-migration/migration/index.migration"
  },
